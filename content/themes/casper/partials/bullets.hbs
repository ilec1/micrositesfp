<style>
  .bullets {
    margin: 30px 0;
  }

  .item-bullet {
    width: 30%;
    margin: 0 1.5%;
    float: left;
  }

  .gh-iconset-icon {
    width: 48px;
    height: 48px;
    margin: 0 auto;
    color: #9d2449;
  }
</style>
<!-- Bullets ================================================== -->
<div id="bullets-home" class="bullets container-fluid">


  {{#get "posts" limit="3" include="tags" filter="tags:bullet-home" order="published_at desc"}}
  {{#foreach posts}}
  <div class="item-bullet">
    <div class="gh-iconset-icon">
      {{#if featured-image}}
      <img src={{featured-image}} alt="{{tile}}" height="45px">
      {{else}}
      <img
        src="data:image/png;base64,"
        alt="{{tile}}" height="45px">
      {{/if}}
    </div>

    <h2 class="text-center">{{title}}</h2>
    <div class="text-center">{{content}}</div>


  </div>
  {{else}}
  {{! If there are no featured posts}}
  <p>No hay nada!</p>
  {{/foreach}}
  {{/get}}

</div>

<!-- /.Bullets -->